<ResourceDictionary 
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <x:Double x:Key="TextControlThemeMinHeight">24</x:Double>
            <Thickness x:Key="TextControlThemePadding">2,1,6,0</Thickness>
            <x:Double x:Key="ListViewItemMinHeight">32</x:Double>
            <x:Double x:Key="TreeViewItemMinHeight">24</x:Double>
            <Thickness x:Key="TimePickerFlyoutPresenterItemPadding">0,2,0,3</Thickness>
            <Thickness x:Key="DatePickerFlyoutPresenterItemPadding">0,2,0,3</Thickness>
            <Thickness x:Key="DatePickerFlyoutPresenterMonthPadding">9,2,0,3</Thickness>
        </ResourceDictionary>

        <ResourceDictionary x:Key="HighContrast">
            <x:Double x:Key="TextControlThemeMinHeight">24</x:Double>
            <Thickness x:Key="TextControlThemePadding">2,1,6,0</Thickness>
            <x:Double x:Key="ListViewItemMinHeight">32</x:Double>
            <x:Double x:Key="TreeViewItemMinHeight">24</x:Double>
            <Thickness x:Key="TimePickerFlyoutPresenterItemPadding">0,2,0,3</Thickness>
            <Thickness x:Key="DatePickerFlyoutPresenterItemPadding">0,2,0,3</Thickness>
            <Thickness x:Key="DatePickerFlyoutPresenterMonthPadding">9,2,0,3</Thickness>
        </ResourceDictionary>

        <ResourceDictionary x:Key="Light">
            <x:Double x:Key="TextControlThemeMinHeight">24</x:Double>
            <Thickness x:Key="TextControlThemePadding">2,1,6,0</Thickness>
            <x:Double x:Key="ListViewItemMinHeight">32</x:Double>
            <x:Double x:Key="TreeViewItemMinHeight">24</x:Double>
            <Thickness x:Key="TimePickerFlyoutPresenterItemPadding">0,2,0,3</Thickness>
            <Thickness x:Key="DatePickerFlyoutPresenterItemPadding">0,2,0,3</Thickness>
            <Thickness x:Key="DatePickerFlyoutPresenterMonthPadding">9,2,0,3</Thickness>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
    <Style TargetType="TextBlock" >
        <Setter Property="FontSize" Value="14" />
    </Style>
</ResourceDictionary>